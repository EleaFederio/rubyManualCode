<h1>There a <%= @players.count %> of players in our league</h1>

<table>
    <thead>
        <tr>
            <td>First Name</td>
            <td>Last Name</td>
        </tr>
    </thead>
    <tbody>
        <% @players.each do |player| %>
        <tr>
            <td><%= player.first_name %></td>
            <td><%= player.last_name %></td>
        </tr>
        <% end%>
    </tbody>
</table>

<a href="/players/new">New Player</a>